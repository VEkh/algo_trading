#! /bin/bash

# ./bin/run ./src/td_ameritrade/td_ameritrade.cpp -lcurl

mkdir -p ./build/

td_ameritrade_sources=("td_ameritrade")

for src in "${td_ameritrade_sources[@]}"
do
  g++ \
    -iquote. \
    -iquote./src \
    -std=c++17 \
    -o ./build/td_ameritrade \
    ./src/td_ameritrade/$src.cpp \
    -lcurl
done
