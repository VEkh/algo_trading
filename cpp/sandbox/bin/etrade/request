# Production
# REQUEST_URL="https://api.etrade.com/oauth/access_token"
# REQUEST_URL="https://api.etrade.com/oauth/renew_access_token"
# REQUEST_URL="https://api.etrade.com/oauth/revoke_access_token"
# REQUEST_URL="https://api.etrade.com/oauth/request_token"
# REQUEST_URL="https://api.etrade.com/v1/accounts/list"
# REQUEST_URL="https://api.etrade.com/v1/market/quote/tsla"
REQUEST_URL="https://api.etrade.com/v1/market/quote/TSLA?detailFlag=ALL"


headers=`REQUEST_URL=$REQUEST_URL ./bin/build_request_header`

curl -H "$headers" $REQUEST_URL
echo
